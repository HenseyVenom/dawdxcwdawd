.staffInfo{
  width:100%;
  box-sizing:border-box;
}
.staffInfo .infolist{
  height:90rpx;
  line-height:90rpx;
  background:#fff;
  font-size:32rpx;
  padding:0rpx 26rpx;
  position:relative; 
  border-bottom:1px solid #ededed;
}
.staffInfo .infolist .rightinfo{
  position:absolute;
  top:0px;
  right:26rpx;
  font-size:30rpx;
  color:#999;
  width:500rpx;
  text-align:right;
}
.staffInfo .infolist .rightinfo .import{
  text-align:right;
  margin-top:20rpx;
  color:#333;
}
.staffInfo .infolist .rightinfo .arrow_right{
  width: 16rpx;
	height: 25rpx;
  margin-left:10px;
  
}
.staffInfo .top{
  margin-top:21rpx;
}
.staffInfo .infodelect{
  width: 625rpx;
	height: 88rpx;
	background-color: #ffffff;
	border-radius: 10rpx;
	border: solid 1rpx #ededed;
  text-align:center;
  line-height:88rpx;
  font-size: 36rpx;
  color: #ff1c1c;
}
.staffInfo .delect{
  margin:139rpx auto;
}
.staffInfo .sureinfo{
  position: fixed;
  bottom:80rpx;
  left:50px;
  margin-left:-44rpx;
  background-color: #f23535;
  font-size: 36rpx;
  color:#fff;
}
.updateNameModel{
  width:100%;
  height:100%;
  position:fixed;
  left:0px;
  top:0px;
  z-index:111;
  background:#f1f1f1;
}
.updateNameModel .personName{
  height:80rpx;
  width:100%;
  background:#fff;
  font-size:26rpx;
  margin-top:10rpx;
  padding-top:10rpx;
}
.updateNameModel .personName input{
  padding:0rpx 10rpx;
  height:60rpx;
  line-height:60rpx;
}
.updateNameModel .bttonArea .sureName{
   width:100rpx;
   height:60rpx;
   line-height:60rpx;
   background-color: #f23535;
	 border-radius:4rpx;
   font-size:26rpx;
   color:#fff;
   text-align:center;
   padding:0px;
   float:right;
   margin:10rpx;
}
.updateNameModel .sex{
  height:90rpx;
  line-height:90rpx;
  background:#fff;
  font-size:32rpx;
  border-bottom:1px solid #ededed; 
  padding-left:30rpx;
  position:relative;
  box-sizing:border-box;
}
.updateNameModel .sex .checked{
  width: 35rpx;
	height: 25rpx;
  position:absolute;
  right:30rpx;
  top:32rpx;  
}
.shopName{
  display: inline-block;
  width: 460rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: middle;
}