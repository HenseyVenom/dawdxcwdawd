.organbanner {
  position: relative;
}

.slide-image {
  width: 100%;
  height: 360rpx;
}

.organswiper .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 2rpx;
  right: 0rpx !important;
  left: unset;
  bottom: 10rpx;
}

.organswiper .wx-swiper-dot {
  display: inline-flex;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  margin-left: 14rpx;
  justify-content: space-between;
  background-color: white;
}

.organswiper .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(255, 255, 255, 0.8);
  border-radius: 8rpx;
}

.organswiper .wx-swiper-dot-active {
  width: 26rpx;
}
