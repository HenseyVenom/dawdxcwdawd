.Mark{
  width:100%;
  height:100%;
  background:rgba(0,0,0,.4);
  position:fixed;
  left:0px;
  top:0px;
}
.Mark .viewContent{
  width:540rpx;
  background:#fff;
  text-align:center;
  font-size:30rpx;
  margin:300rpx auto;
  border-radius:8rpx;
}
.Mark .viewContent .markTitle{
  padding:40rpx 60rpx;  
}
.Marksure{
  color: #F23535;
  height:90rpx;
  font-size:36rpx; 
  border-top:1px solid #E5E5E5;
  line-height:90rpx;
}